<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="scene-default" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="imat.ui.views.browse.Browse">
    <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
                  <fx:include source="../../controls/header/header.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <StackPane prefHeight="150.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                   <fx:include fx:id="checkoutPane" source="checkout/checkout.fxml" />
                    <GridPane fx:id="browseGridPane">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" percentWidth="25.0" prefWidth="300.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" prefWidth="400.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" percentWidth="25.0" prefWidth="300.0" />
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" valignment="TOP" vgrow="ALWAYS" />
                        </rowConstraints>
                        <children>
                            <AnchorPane fx:id="middlePane" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                                <children>
                                    <StackPane fx:id="sceneStackPane" minWidth="0.0" onMouseClicked="#consumeEvent" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                        <fx:include fx:id="productPane" source="centerviews/products/products.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <fx:include fx:id="historyPane" source="centerviews/history/orders/order_history.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <fx:include fx:id="historyArticlesPane" source="centerviews/history/articles/history_articles.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                        <fx:include fx:id="homePane" source="centerviews/home/home.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </StackPane>
                                </children>
                            </AnchorPane>
                            <AnchorPane prefHeight="200.0" prefWidth="150.0" styleClass="debug-background-lightgreen">
                                <children>
                                     <fx:include source="sidebars/category/category_sidebar.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                </children>
                            </AnchorPane>
                            <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="debug-background-lightyellow" GridPane.columnIndex="2">
                                <children>
                                    <fx:include fx:id="cartSidebar" source="sidebars/cart/cart_sidebar.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                </children>
                            </AnchorPane>
                        </children>
                    </GridPane>
               </children>
            </StackPane>
         </children>
      </VBox>
    </children>
   <stylesheets>
      <URL value="@../../../resources/css/imat.css" />
      <URL value="@../../../resources/css/debug.css" />
      <URL value="@../../../resources/css/serene-colors.css" />
   </stylesheets>
</AnchorPane>
